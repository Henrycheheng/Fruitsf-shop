/* components/modal/modal.wxss */
.inputBox-container {
  position: fixed;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 100vh;
  z-index: 998;
}
.shadow {
  position: absolute;
  top: 0;
  left: 0;
  background-color: black;
  width: 750rpx;
  height: 100vh;
  z-index: 998;
}

.modal {
  display: flex;
  flex-direction: column;
  position: absolute;
  left: 40rpx;
  background-color: #F5F5F5;
  width: 670rpx;
  /* height: 706rpx; */
  border-radius: 9rpx;
  z-index: 998;
}

.title {
  padding: 68rpx 0 60rpx 0;
  color: #656565;
  font-size: 15px;
  text-align: center;
}


@keyframes shadowDisplay {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 0.65;
  }
}

@keyframes modalDisplay {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }
}

@keyframes shadowHide {
  0% {
    opacity: 0.65;
  }

  100% {
    opacity: 0;
  }
}

@keyframes modalHide {
  0% {
    opacity: 1;
  }

  100% {
    opacity: 0;
  }
}

